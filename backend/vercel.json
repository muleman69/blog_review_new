{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/debug",
      "dest": "/dist/index.js"
    },
    {
      "src": "/api/health",
      "dest": "/dist/index.js"
    },
    {
      "src": "/api/blog-posts(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ]
} 