{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/debug",
      "dest": "src/index.ts"
    },
    {
      "src": "/api/health",
      "dest": "src/index.ts"
    },
    {
      "src": "/api/blog-posts(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
} 