# API Configuration
VITE_API_URL=http://localhost:3001
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Feature Flags
VITE_ENABLE_AI_SUGGESTIONS=true
VITE_ENABLE_REAL_TIME_VALIDATION=true

# Analytics and Monitoring
VITE_ENABLE_ANALYTICS=false
